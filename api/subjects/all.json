---
layout: compress
---

{% assign subjects = "" | split: ", " %}
{% assign subjectCodes = "" | split: ", " %}
{% assign resources = "" | split: ", " %}
{% assign semesters = "" | split: ", " %}
{% for page in site.pages %}
{% if page.semester %}
{% assign eachSubject = page.title | split: ", " %}
{% assign eachSubjectCode = page.subjectCode| split: ", " %}
{% assign eachResource = page.resources | jsonify | split: ", " %}
{% assign eachSemester = page.semester | replace: " Semester", "" | downcase |  split: ", " %}
{% assign subjects = subjects | concat: eachSubject %}
{% assign subjectCodes = subjectCodes | concat: eachSubjectCode %}
{% assign resources = resources | concat: eachResource %}
{% assign semesters = semesters | concat: eachSemester %}
{% endif %}
{% endfor %}
{
"timestamp":"{{site.time}}",
"subjects":[
{% for subject in subjects %}
{
"name":"{{ subject }}",
"subjectCode":"{{ subjectCodes[forloop.index0] }}",
"semester":"{{ semesters[forloop.index0] }}",
"resources":{{ resources[forloop.index0] }}
}
{% unless forloop.last %},{% endunless %}
{% endfor %}
]
}